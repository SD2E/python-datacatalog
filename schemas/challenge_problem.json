      },
      "project": {
        "description": "Project within specified TACC.cloud tenant",
        "readOnly": true,
        "type": "string",
        "value": "SD2"
      },
      "tenant": {
        "description": "TACC.cloud tenant",
        "enum": [
          "3dem",
          "designsafe",
          "iplantc.org",
          "irec",
          "portals",
          "vdjserver.org",
          "sd2e",
          "sgci",
          "tacc.prod"
